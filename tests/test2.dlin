<?xml version="1.0" encoding="UTF-8"?>
<BellerophonParameters>

	<LogicArguments>
		<Symbols>x y z</Symbols>
		<Equation>(~x &amp; ~y &amp; ~z) | ( ~x &amp; ~y &amp; z)</Equation>		
	</LogicArguments>	

	<Topology>
		<Display_Topology>no</Display_Topology>
		<File_Extension>.png</File_Extension>		
	</Topology>
	
	<ModelChecking>
		<PathToPrism>prism-4.1.beta2-linux64/bin/prism</PathToPrism>
		<InitialiseLength>2</InitialiseLength>
		<MaxErrTolerance>0.10</MaxErrTolerance> 
	</ModelChecking>

	<ProximityErrors>
		<ProximityErrTolerance> </ProximityErrTolerance>
		<OneSpace> </OneSpace>
		<TwoSpace> </TwoSpace>
		<ThreeSpace> </ThreeSpace>
	</ProximityErrors>

	
</BellerophonParameters>

